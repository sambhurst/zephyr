/dts-v1/;

#include <microchip/mec172xnsz.dtsi>

/ {
	model = "Microchip MEC172XEVB_ASSY6906 evaluation board";
	compatible = "microchip,mec172xevb_assy6906", "microchip,mec172xnsz";

	chosen {
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
		zephyr,console = &uart1;
	};
};

&cpu0 {
	clock-frequency = <96000000>;
};

&systick {
	status = "okay";
};

&pcr {
	status = "okay";
};

&uart1 {
	status = "okay";
};
